@model StayLive.Models.Home.HomeVM
<div class="row m-t-20">
    <div class="col-lg-12 col-md-12">
        <div class="card">
            <img src="~/assets/images/profile-bg.jpg" />
            <div class="card-body little-profile text-center">
                <div class="pro-img"><img src="@Url.DrawImage("u", new string[] { "Id", SessionHelper.AccountId.ToString() })"></div>
                <h3 class="m-b-0">@Model.Name</h3>
                <p>@Model.Email</p>
            </div>
        </div>
    </div>
</div>